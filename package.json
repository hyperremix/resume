{
  "name": "resume",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "test": "jest",
    "start:prod": "yarn run build && serve -s build",
    "test:generators": "ts-node ./internals/testing/generators/test-generators.ts",
    "checkTs": "tsc --noEmit",
    "eslint": "eslint --ext js,ts,tsx",
    "lint": "yarn run eslint src",
    "lint:fix": "yarn run eslint --fix src",
    "generate": "plop --plopfile internals/generators/plopfile.ts",
    "prettify": "prettier --write",
    "prepare": "husky install"
  },
  "dependencies": {
    "@emotion/babel-plugin": "11.11.0",
    "@emotion/react": "11.10.5",
    "@emotion/styled": "11.10.5",
    "@fontsource/roboto": "4.5.8",
    "@mdi/js": "7.1.96",
    "@mdi/react": "1.6.1",
    "@mui/icons-material": "5.11.0",
    "@mui/material": "5.11.0",
    "@mui/system": "5.14.14",
    "@reduxjs/toolkit": "1.8.5",
    "dayjs": "1.11.7",
    "fontfaceobserver": "2.3.0",
    "i18next": "21.9.2",
    "i18next-browser-languagedetector": "6.1.5",
    "i18next-scanner": "4.0.0",
    "imagekitio-react": "3.0.0",
    "mdi-material-ui": "7.7.0",
    "react": "18.2.0",
    "react-app-polyfill": "3.0.0",
    "react-dom": "18.2.0",
    "react-helmet-async": "1.3.0",
    "react-i18next": "11.18.6",
    "react-is": "18.2.0",
    "react-redux": "7.2.8",
    "react-router-dom": "6.5.0",
    "react-test-renderer": "18.2.0",
    "redux-injectors": "2.1.0",
    "redux-saga": "1.2.1",
    "styled-components": "5.3.5",
    "symbol-observable": "4.0.0",
    "web-vitals": "2.1.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@types/fontfaceobserver": "^2.1.0",
    "@types/jest": "29.5.6",
    "@types/node": "^14.18.27",
    "@types/react": "^18.0.20",
    "@types/react-dom": "^18.0.6",
    "@types/react-redux": "^7.1.24",
    "@types/react-test-renderer": "^18.0.0",
    "@types/rimraf": "^3.0.2",
    "@types/shelljs": "^0.8.11",
    "@types/styled-components": "^5.1.26",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@types/webpack": "^5.28.0",
    "@types/webpack-env": "^1.18.0",
    "@vitejs/plugin-react": "4.1.0",
    "browserslist-to-esbuild": "1.2.0",
    "chalk": "4.1.2",
    "cross-env": "7.0.3",
    "eslint": "8.52.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-react-app": "7.0.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "8.0.1",
    "inquirer": "7.3.3",
    "inquirer-directory": "2.2.0",
    "jest": "29.7.0",
    "jest-styled-components": "7.1.1",
    "lint-staged": "13.0.3",
    "node-plop": "0.26.3",
    "plop": "2.7.6",
    "prettier": "2.7.1",
    "rimraf": "3.0.2",
    "sanitize.css": "13.0.0",
    "serve": "14.0.1",
    "shelljs": "0.8.5",
    "stylelint": "14.12.0",
    "stylelint-config-recommended": "9.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.10.0",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "typescript": "4.6.4",
    "vite": "4.5.0",
    "vite-tsconfig-paths": "4.2.1"
  },
  "engines": {
    "node": ">=14.x"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "yarn run eslint --fix"
    ],
    "*.{md,json}": [
      "prettier --write"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*/*.d.ts",
      "!src/**/*/Loadable.{js,jsx,ts,tsx}",
      "!src/**/*/messages.ts",
      "!src/**/*/types.ts",
      "!src/index.tsx"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    }
  }
}
